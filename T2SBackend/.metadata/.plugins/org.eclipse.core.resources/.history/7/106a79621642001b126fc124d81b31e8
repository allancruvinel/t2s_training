package com.t2s.entities;

import java.time.LocalDate;
import java.util.Date;

import javax.persistence.Entity;

import com.t2s.enums.CategoryContainer;
import com.t2s.enums.Handling;

@Entity
public class MovementReport {
		

	private String client;
	private CategoryContainer category;
	public Handling typeMovement;
	private String ship;
	private Date startmovement;
	private Date endmovement;
	
	public MovementReport() {}

	
	
	public String getClient() {
		return client;
	}

	public void setClient(String client) {
		this.client = client;
	}

	public CategoryContainer getCategory() {
		return category;
	}

	public void setCategory(CategoryContainer category) {
		this.category = category;
	}

	public Handling getTypeMovement() {
		return typeMovement;
	}

	public void setTypeMovement(Handling typeMovement) {
		this.typeMovement = typeMovement;
	}

	public String getShip() {
		return ship;
	}

	public void setShip(String ship) {
		this.ship = ship;
	}

	public Date getStartmovement() {
		return startmovement;
	}

	public void setStartmovement(Date startmovement) {
		this.startmovement = startmovement;
	}

	public Date getEndmovement() {
		return endmovement;
	}

	public void setEndmovement(Date endmovement) {
		this.endmovement = endmovement;
	}

	
	
	

	
	
	

	
	
	
}


