package com.t2s.entities;

public class Ship {
	private Long id;
	private String name;
	
	public Ship() {}
	
	public Ship(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Long getId() {
		return id;
	}
	
	
}
